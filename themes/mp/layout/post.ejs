<main style="background-color:#c5cae9;">

    <link rel="stylesheet" href="<%= theme.libs.tocbot.css %>">
    <link rel="stylesheet" href="<%= theme.postPage.css.post %>">
    <link rel="stylesheet" href="<%= theme.postPage.css.main %>">
    
    <header>
        <%- partial('_partial/index_header') %>
        <%- partial('_partial/post_banner_common') %>
    </header>

    <div class="row" style="margin-top:30px;">
        <div class="col s1 m1 l1"></div>
        <div class="col s10 m10 l10" style="padding:0;">
            <div class="row">
                <div id="js-toc-content" class="col s12 m12 l9 card" style="padding:10px 20px;background-color:rgba(241, 240, 238,0.2);">
                    <%- page.content %>
                </div>
                <div id="tocbot" class="col s0 m0 l3 card" style="background:red;height:200px;">
                    <%- partial('_widget/tocbot') %>
                </div>
            </div>
        </div>
        <div class="col s1 m1 l1"></div>
    </div>

    <script type="text/javascript" src="<%= theme.libs.tocbot.js %>"></script>
    <script type="text/javascript" src="<%= theme.postPage.js.commonBanner %>"></script>
    <script type="text/javascript" src="<%= theme.postPage.js.main %>"></script>
    <script type="text/javascript" src="<%= theme.postPage.js.mainCss %>"></script>
</main>