<%# nav%>
<div class="navbar-fixed">
    <% var navColor = "#7b1fa2",navTextColor = "#fff";
    if(theme.nav.enable){
        if(theme.nav.color != null){
            navColor = "#" + theme.nav.color;
        }
        if(theme.nav.textColor != null){
            navTextColor = "#" + theme.nav.textColor;
        }
    } %>
    <nav style="background-color:<%= navColor %>;">
        <div class="row">
            <div class="col s1 m1 l1"></div>
            <div class="col s10 m10 l10">
                <div class="nav-wrapper">
                    <a href="<%= config.url %>" class="brand-logo">Logo</a>
                    <ul class="right hide-on-med-and-down">
                        <% Object.keys(theme.menu).forEach( item =>{ %>
                        <li>
                            <a href="<%= theme.menu[item].url %>" style="color:<%= navTextColor %>;">
                                <% if(theme.menu[item].icon != null){ %>
                                <i class="fa fa-<%= theme.menu[item].icon%>"></i> 
                                <% } %>
                                <%= __(item) %>
                            </a>
                        </li>
                        <% });%>
                    </ul>
                    <a href="#" data-activates="slide-out" class="button-collapse"><i class="material-icons">menu</i></a>
                </div>
            </div>
            <div class="col s1 m1 l1"></div>
        </div>
        
    </nav>
</div>
<%- partial('_partial/index_header_phone') %>